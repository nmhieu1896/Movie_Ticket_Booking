{
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  "extends": ["plugin:prettier/recommended", "airbnb"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "linebreak-style": 0,
    "spaced-comment": "off",
    "import/first": "off",
    "no-unused-vars": "warn"
  }
}